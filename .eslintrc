{
	"env": {
		"browser": true
	},
	"rules": {
		"camelcase": 0,
		"consistent-return": 0,
		"eqeqeq": 0,
		"new-cap": 0,
		"no-shadow": 0,
		"no-trailing-spaces": 0,
		"no-use-before-define": [2, "nofunc"],
		"no-unused-vars": [2, {"vars": "local"}],
		"quotes": 0,
		"strict": 0
	},
	"globals": {
		"GM_registerMenuCommand": false,
		"GM_getValue": false,
		"GM_setValue": false,
		"BezierEasing": false
	}
}
